    <script>
      function checkPhone(value) {
        let phoneNum =
          /^[\+]?[(]?[0-9]{3}[)]?[-\s\.]?[0-9]{3}[-\s\.]?[0-9]{4,6}$/im;
        return value.match(phoneNum);
      }
      function isEmailAddress(str) {
        let pattern = /^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/;
        return pattern.test(str);
      }
      function isName(str) {
        let pattern = /^[a-zA-z]+([\s][a-zA-Z]+)*$/;
        return pattern.test(str);
      }
      function handleSelect(e){
        e.target['id']==="firstTimeGmat"?
          document.getElementById("retakeGmat").checked=false :
          document.getElementById("firstTimeGmat").checked=false
      }
      function handleOnSubmit(e) {
        //e.preventDefault();
        const urlParams = new URLSearchParams(window.location.search);
        let url, source, campaign, medium, device, term, name, email, phone,gmatAppearingStatus;
        url = window.location.href;
        source = urlParams.get("utm_source");
        campaign = urlParams.get("utm_campaign");
        medium = urlParams.get("utm_medium");
        term = urlParams.get("utm_term");
        device = urlParams.get("utm_device");
        name = document.getElementById("name");
        email = document.getElementById("email");
        phone = document.getElementById("phone");
        gmatAppearingStatus = "Interested In GMAT";
        if(document.getElementById("firstTimeGmat").checked){
          gmatAppearingStatus = "First Time GMAT";
        }else if(document.getElementById("retakeGmat").checked){
          gmatAppearingStatus = "Retaking GMAT";
        }
        const data = {
          account: "GMAT",
          firstName: name.value.split(" ")[0],
          lastName:
            name.value.split(" ")[1] !== undefined
              ? name.value.split(" ")[1]
              : "",
          email: email.value,
          phone: phone.value,
          pageCode: "255",
          source: source,
          sourceCode: campaign,
          website: url,
          sourceMedium: medium,
          device: device,
          term: term,
          eventMsg: `**[${gmatAppearingStatus}] ${name.value.split(" ")[0]} registered from ${url} Website.`,
        };
        console.log(data);
        document.getElementsByClassName("submit-btn")[0].value = "Loading...";
        document.getElementsByClassName("submit-btn")[0].disabled = true;
        fetch("https://careerwizard.thecareerlabs.com/ls/api/v1/registration", {
          method: "POST",
          headers: {
            "Content-Type": "application/json",
          },
          body: JSON.stringify(data),
        }).then((response) => {
            // console.log(response);
          localStorage.setItem("lp-form-submission", "success");
          document.getElementsByClassName("submit-btn")[0].value = "Start My Prep";
          document.getElementsByClassName("submit-btn")[0].disabled = false;
          })
          .catch((error) => {
            console.error("Error:", error);
          });
        fetch(
          "https://careerwizard.thecareerlabs.com/verloop/Campaign/SendMessage",
          {
            method: "POST",
            headers: {
              "Content-Type": "application/json",
            },
            body: JSON.stringify({
              CampaignID: "b58158c1-19d7-47bb-b325-7b03c6583a45",
              PhoneNumber: `91${data.phone}`,
            }),
          }
        )
          .then((response) => {
            // console.log(response)
          })
          .catch((error) => {
            // console.error("Error:", error);
          });
      }
    </script>
